-
    const links = [
        {
            href: "https://vitejs.dev",
            imgSrc: "/assets/svg/vite.svg",
            imgAlt: "Vite logo"
        },
        {
            href: 'https://www.typescriptlang.org',
            imgSrc: '/assets/svg/typescript.svg',
            imgAlt: 'TypeScript logo'
        },
        {
            href: 'https://sass-lang.com/',
            imgSrc: '/assets/svg/sass.svg',
            imgAlt: 'SASS logo'
        },
        {
            href: 'https://pugjs.org/',
            imgSrc: '/assets/svg/pug.svg',
            imgAlt: 'PUG logo'
        },
    ];

#app
    each link in links
        a(href=link.href, target='_blank')
            img.logo(src=link.imgSrc, alt=link.imgAlt)

    h1 Vite + TypeScript + SASS + PUG

    .card
        button#counter(type='button')

    p.read-the-docs Click on the logos to learn more
